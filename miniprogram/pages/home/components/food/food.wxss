/* pages/home/components/foodComponent/foodComponent.wxss */
.food{
  height: 100%;
  background-color: #f7f8fa;
}
.food-card{
  width: 94%;
  margin: auto;
  box-shadow: 0 8px 12px #ebedf0;
  border-radius: 10rpx;
}
.img-list{
  display: flex;
  overflow: hidden;
  justify-content: space-between;
}
.img-list .img-list-item{
  width: 32%;
  box-sizing: border-box;
  overflow: hidden;
}
.img-list .img-list-item image{
  width: 100%;
  vertical-align: top;
}
.f-p-l{
  display: flex;
  font-size: 30rpx;
  justify-content: space-between;
  padding:10rpx 40rpx;
}
